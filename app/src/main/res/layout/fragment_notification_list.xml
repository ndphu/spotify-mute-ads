<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.SwipeRefreshLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/swiperefresh"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
  <android.support.v7.widget.RecyclerView
      android:id="@+id/list"
      android:name="vn.com.phudnguyen.tools.autovolumemanager.listener.fragment.notification.NotificationListFragment"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      app:layoutManager="android.support.v7.widget.LinearLayoutManager"
      tools:context=".listener.fragment.notification.NotificationListFragment"
      tools:listitem="@layout/fragment_notification"/>
</android.support.v4.widget.SwipeRefreshLayout>